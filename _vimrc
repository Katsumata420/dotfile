syntax on
set number
set showmatch
set autoindent
set hlsearch
set tabstop=4
set expandtab
set shiftwidth=4
set mouse=a
set laststatus=4
set textwidth=0
set cursorline
set title
set smartindent
inoremap jj <Esc>
inoremap <C-e> <Esc>$a
inoremap <C-a> <Esc>^a
set encoding=utf-8
set clipboard+=autoselect
set clipboard+=unnamed
nnoremap <C-o> : <C-u>setlocal cursorcolumn!<CR>
"set list
